<!DOCTYPE html>
<html>
<head>
    <title>Freelancer - Start Bootstrap Theme</title>
      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
      <%= csrf_meta_tags %>
    <!-- Bootstrap Core CSS -->
    <link href="../vendor/assets/bootstrap.min.css" rel="stylesheet">
    <!-- Theme CSS -->
    <link href="../../assets/freelancer.min.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
<!-- Custom Fonts -->
    <link href="../vendor/assets/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">
</head>
<body id="page-top" class="index">
        <%= render '/layouts/navigation_bar'%>
		<%= render '/layouts/header'%>
		<%= render '/layouts/alerts'%>       
				<%= yield %>		
		<%= render '/layouts/footer'%>
        <!-- jQuery -->
    <script src="../vendor/assets/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="../vendor/assets/bootstrap.min.js"></script>
    <!-- Plugin JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <!-- Contact Form JavaScript -->
    <script src="../../assets/jqBootstrapValidation.js"></script>
    <script src="../../assets/contact_me.js"></script>
    <!-- Theme JavaScript -->
    <script src="../../assets/freelancer.min.js"></script>
</body>
</html>
